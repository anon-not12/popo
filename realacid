<script>
    var container = document.getElementById('result-container');
    passwordEntered = false

    function programWebsite(code) {
        if (code == ''){
            container.src = "";
            searchQuery.value = "";
            passwordEntered = false
        }else{
          if (!code.includes('http')){
            code = 'https://'+code;
          }
          container.src = code;
        } 
    }

    function hashSame(text){
      return dcodeIO.bcrypt.compare(text,hashedPass)
    }

    function promptSearch() {
        let text = prompt("What would you like to learn about Ocean Acidification? ");
        if(!passwordEntered){
          hashSame(text).then((passMatched) => {
            if (passMatched == true){
              passwordEntered = true;
              alert("Accepted");
            }
          });
        }
        else if(passwordEntered){
          if (text == null || text == "") {
            programWebsite("");
          } else {
              programWebsite(text)
          }
        }
    }
</script>
